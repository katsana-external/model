{
    "name": "orchestra/model",
    "description": "Model Component for Orchestra Platform",
    "homepage": "http://orchestraplatform.com/docs/latest/components/model",
    "keywords": [
        "orchestra-platform",
        "orchestral",
        "model",
        "laravel"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Model\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orchestra\\Model\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "laravie/dhosa": "2.x-dev",
        "orchestra/contracts": "^7.0",
        "orchestra/database": "7.x-dev",
        "orchestra/support": "^7.0"
    },
    "require-dev": {
        "orchestra/auth": "7.x-dev",
        "orchestra/memory": "^7.0",
        "orchestra/testbench": "^7.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/auth"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/dhosa"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/database"
        }
    ]
}
